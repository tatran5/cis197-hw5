oProps = state => ({\r\n  imageURL: state.intro.imageURL,\r\n\tdescription: state.intro.description\r\n})\r\n\r\n\r\nconst mapDispatchToProps = dispatch => ({\r\n\tdispatchEditIntro: (imageURL, description) => dispatch(editIntro(imageURL, description))\r\n})\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(IntroEditor)"},"lineCount":null}},"error":null,"hash":"f3e250822bfdd54c572bbc58187ec570","cacheData":{"env":{}}}